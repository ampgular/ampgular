{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "itemListElement"
  ],
  "properties": {
    "itemListElement":  { 
      "type": "array",
      "title": "The Itemlistelement Schema",
      "items": {
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "position",
          "name",
          "item"
        ],
        "properties": {
          "position": {
            "type": "integer",
            "title": "The Position"
         
          },
          "name": {
            "type": "string",
            "title": "The Name"
          
          },
          "item": {
           "type": "string",
            "title": "The URL of the item"
          }
        }
      }
    }
  }
}