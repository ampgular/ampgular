  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ampgular/ampgular/issues"
  },
  "homepage": "https://github.com/ampgular/ampgular",
  "workspaces": {
    "packages": [
      "packages/ampgular/*",
      "packages/angular_devkit/*"
    ]
  },
  "dependencies": {
